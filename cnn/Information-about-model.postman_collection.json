		{
			"name": "Negotiate a contract",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Retrieve contract negotiation\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.collectionVariables.set(\"contract negotiation\", jsonData[\"@id\"])",
							"});"
